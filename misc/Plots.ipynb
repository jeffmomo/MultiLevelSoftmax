    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "\n",
    "time = np.arange(-10.0, 10.0, 0.01)\n",
    "\n",
    "\n",
    "relu = np.maximum(time, 0.0)\n",
    "sigmoid = 1 / (1 + np.exp(-time))\n",
    "tanh = 2 * (1 / (1 + np.exp(-2 * time))) - 1\n",
    "\n",
    "\n",
    "plt.plot(time, relu, time, sigmoid, time, tanh)\n",
    "plt.show()"
   ]
  }
